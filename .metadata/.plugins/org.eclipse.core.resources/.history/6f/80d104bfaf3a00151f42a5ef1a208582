		case hochschuleemf.diagram.edit.parts.HochschuleEditPart.VISUAL_ID:
	return getHochschule_1000Text(view);
case hochschuleemf.diagram.edit.parts.FachbereichEditPart.VISUAL_ID:
	return getFachbereich_2001Text(view);
case hochschuleemf.diagram.edit.parts.StudiengangEditPart.VISUAL_ID:
	return getStudiengang_3001Text(view);
case hochschuleemf.diagram.edit.parts.HochschuleFachbereichEditPart.VISUAL_ID:
	return getHochschuleFachbereich_4001Text(view);
case hochschuleemf.diagram.edit.parts.FachbereichStudiengangEditPart.VISUAL_ID:
	return getFachbereichStudiengang_4002Text(view);
	}
		return getUnknownElementText(view);
}
	
